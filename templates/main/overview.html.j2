<div id="home" class="navbar-buffer">
</div>
<div class="bg" style="background-image: url({{ overview.backgroundImageUrl }})">
  <div class="text-overlay">
    <h3>{{ overview.firstQuote }}</h3>
    <p>{{ overview.firstQuoteAuthor }}</p>
  </div>
  <div class="arrow animated bounce" onclick="autoScrollTo('profile');">
    <img width="40" height="40" alt=""
         src="data:image/svg+xml;base64,"/>
  </div>
</div>

<div class="fluid-container">
  <div id="profile" class="navbar-buffer">
  </div>
  <div class="container">
    <div class="row text-center">
      <h2>Profile</h2>
    </div>

    <div class="row">
      <div class="col-md-4">
        <h4 class="text-center">Summary</h4>
        <p class="lead">{{ overview.summary }}</p>
      </div>
      <div class="col-md-4">
        <img src="{{ overview.profilePictureUrl }}" style="max-width:250px " id="profil_pic"
             class="img-responsive img-circle center-block" alt="Responsive image">

        <div class="second-quote">
          <h4>{{ overview.secondQuote }}</h4>
          <p>{{ overview.secondQuoteAuthor }}</p>
        </div>
      </div>
      <div class="col-md-4 text-center">
        <h3>Details</h3>
        <div>
          <h5 style="margin-bottom: 5px;">From</h5>
          <em>{{ overview.details.from}}</em>

          <h5 style="margin-bottom: 5px;">Living in</h5>
          <em>{{ overview.details.livingIn}}</em>

          <h5 style="margin-bottom: 5px;">Age</h5>
          <em><span id="age">27</span> years old</em>
        </div>
        <ul id="social-links" style="list-style-type: none;">
          {% for socialLink in overview.socialLinks %}
            <li class="social-link">
              <a href="{{ socialLink.urlLink }}" target="_blank">
                <img class="social-link-img" src="{{ socialLink.logo }}">
              </a>
            </li>
          {% endfor %}
        </ul>
      </div>
    </div>
  </div>
</div>